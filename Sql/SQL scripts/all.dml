CREATE OR REPLACE FUNCTION pocet_zamestnancu_na_pozici(p_idpozice IN NUMBER) RETURN NUMBER IS
    v_count NUMBER;
BEGIN
    SELECT COUNT(*)
    INTO v_count
    FROM zamestnanci
    WHERE pozice_idpozice = p_idpozice;

    RETURN v_count;
EXCEPTION
    WHEN NO_DATA_FOUND THEN
        RETURN 0;
    WHEN OTHERS THEN
        RAISE;
END pocet_zamestnancu_na_pozici;
/

create or replace FUNCTION validne_rc 
(
  RC IN NUMBER  
) RETURN NUMBER AS

  i_zbytek NUMBER;
  i_mesic NUMBER;
  i_rok NUMBER;
  i_den NUMBER;
  i_datum DATE;

BEGIN

  IF LENGTH(RC) NOT BETWEEN 9 AND 10 THEN
    RETURN 0;
  END IF;

  -- rodne cislo pred rokem 1954
  IF LENGTH(RC) = 9 AND SUBSTR(RC,1,2) < 54 THEN
    RETURN 1;
  END IF;


  i_zbytek := MOD(SUBSTR(RC,1,9),11);

  IF i_zbytek = 10 THEN
    i_zbytek := 0;
  END IF;

  i_rok := TO_NUMBER(SUBSTR(RC,1,2));
  i_mesic := TO_NUMBER(SUBSTR(RC,3,2));
  i_den := TO_NUMBER(SUBSTR(RC,5,2));

  IF i_rok < 54 THEN
    i_rok := i_rok + 2000;
  ELSE
    i_rok := i_rok + 1900;
  END IF;


  IF i_mesic > 70 AND i_rok > 2003 THEN
    i_mesic := i_mesic - 70;
  ELSIF i_mesic > 50 THEN
    i_mesic := i_mesic - 50;
  ELSIF i_mesic > 20 AND i_rok > 2003 THEN
    i_mesic := i_mesic - 20;
  END IF;

  BEGIN
    i_datum := TO_DATE(i_den || '.' || i_mesic || '.' || i_rok,'DD.MM.YYYY');
  EXCEPTION WHEN OTHERS THEN
    RETURN 0;
  END;


  IF i_zbytek = SUBSTR(RC,10,1) THEN
    RETURN 1;
  ELSE
    RETURN 0;
  END IF;

END validne_rc;
/
CREATE OR REPLACE FUNCTION get_dostupnost_zbozi(id_produktu IN zbozi.idzbozi%TYPE) RETURN VARCHAR2 AS
  dostupnost_info VARCHAR2(4000);
  celkove_mnozstvi NUMBER;
  nazev_produktu VARCHAR2(32);
BEGIN
 
    SELECT z.nazev
    INTO nazev_produktu
    FROM zbozi z
    WHERE z.idzbozi = id_produktu;

    SELECT SUM(zns.pocetzbozi)
    INTO celkove_mnozstvi
    FROM zbozi_na_sklade zns
    WHERE zns.zbozi_idzbozi = id_produktu;

    IF celkove_mnozstvi IS NULL THEN
        dostupnost_info := 'Produkt ' || nazev_produktu  || ' není aktuálně dostupný ve skladech.';
    ELSE
        dostupnost_info := 'Produkt ' || nazev_produktu || ' je dostupný ve skladech s celkovým množstvím: ' || TO_CHAR(celkove_mnozstvi);
    END IF;

    RETURN dostupnost_info;
EXCEPTION
    WHEN NO_DATA_FOUND THEN
        RETURN 'Nebyly nalezeny žádné informace pro produkt ID: ' || TO_CHAR(id_produktu);
    WHEN OTHERS THEN
        RETURN 'Došlo k chybě při získávání informací o produktu.';
END get_dostupnost_zbozi;
/
CREATE OR REPLACE PROCEDURE smazat_autuzivatele(p_IDAUTUZ IN NUMBER) AS
BEGIN
    delete from autuzivatel_role where autuzivatele_idautuz = p_IDAUTUZ;

    delete from soubory where autuzivatele_idautuz = p_IDAUTUZ;

    delete from autuzivatele where idautuz = p_IDAUTUZ;
    COMMIT;
END;
/

CREATE OR REPLACE PROCEDURE insert_produkt (
  id_produktu IN NUMBER,
  mnozstvi_zmeny IN NUMBER,
  id_skladu IN NUMBER
) AS
BEGIN
  MERGE INTO zbozi_na_sklade USING dual ON (zbozi_idzbozi = id_produktu AND sklady_idskladu = id_skladu)
  WHEN MATCHED THEN
    UPDATE SET pocetzbozi = pocetzbozi + mnozstvi_zmeny
  WHEN NOT MATCHED THEN
    INSERT (zbozi_idzbozi, sklady_idskladu, pocetzbozi)
    VALUES (id_produktu, id_skladu, mnozstvi_zmeny);

  COMMIT;
END insert_produkt;
/

CREATE OR REPLACE PROCEDURE delete_starsi_logy (
  dny_stare IN NUMBER
) AS
  datum_vymezene DATE;
BEGIN
  datum_vymezene := SYSDATE - dny_stare;

  DELETE FROM logy
  WHERE cas < datum_vymezene;

  COMMIT;
END delete_starsi_logy;
/
CREATE OR REPLACE PROCEDURE update_cena_zbozi_kategorii (
  kategorie_id IN kategorije.idkategorije%TYPE,
  procento_navyseni IN NUMBER
) AS
  CURSOR kurzor_produktu IS
    SELECT idzbozi, aktualnicena
    FROM zbozi
    WHERE kategorije_idkategorije = kategorie_id;

  v_product_id NUMBER;
  v_aktualni_cena NUMBER;

BEGIN
  OPEN kurzor_produktu;

  LOOP
    FETCH kurzor_produktu INTO v_product_id, v_aktualni_cena;
    EXIT WHEN kurzor_produktu%NOTFOUND;

    UPDATE zbozi
    SET aktualnicena = v_aktualni_cena * (1 + procento_navyseni / 100)
    WHERE idzbozi = v_product_id;

  END LOOP;

  CLOSE kurzor_produktu;

  COMMIT;
END update_cena_zbozi_kategorii;
/
