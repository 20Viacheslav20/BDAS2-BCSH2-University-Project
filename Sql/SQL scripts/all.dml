		v_zmeny := v_zmeny || ' | ' || 'jmeno ' || '' || :old.jmeno || ' -> ' || 'jmeno ' || ' ' || :new.jmeno; 
	END IF; 
	IF (:new.prijmeni != :old.prijmeni) THEN 
		v_zmeny := v_zmeny || ' | ' || 'prijmeni ' || '' || :old.prijmeni || ' -> ' || 'prijmeni ' || ' ' || :new.prijmeni; 
