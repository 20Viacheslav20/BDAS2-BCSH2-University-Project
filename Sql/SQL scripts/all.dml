	IF (:new.rodnecislo != :old.rodnecislo) THEN 
		v_zmeny := v_zmeny || ' | ' || 'rodnecislo ' || '' || :old.rodnecislo || ' -> ' || 'rodnecislo ' || ' ' || :new.rodnecislo; 
